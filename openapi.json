{
    "openapi": "3.1.0",
    "info": {
      "title": "ClickUp Connector",
      "version": "1.0.0",
      "description": "Fetches tasks from your ClickUp account"
    },
    "servers": [
      {
        "url": "https://clickup-connector.onrender.com/"
      }
    ],
    "paths": {
      "/tasks": {
        "get": {
          "operationId": "getTasks",
          "summary": "Fetch tasks from ClickUp",
          "responses": {
            "200": {
              "description": "A list of tasks from your ClickUp list"
            }
          }
        }
      }
    }
  }